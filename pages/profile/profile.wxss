.header {
  position: fixed;
  top: 80px;
  left: 0;
  right: 0;
  height: 40px;
  display: flex;
  align-items: center;
  padding: 0 15px;
  z-index: 100;
}

.header-left {
  position: absolute;
  left: 15px;
}

.return-icon {
  width: 24px;
  height: 24px;
}

.title {
  flex: 1;
  text-align: center;
  font-size: 16px;
  color: #333;
}